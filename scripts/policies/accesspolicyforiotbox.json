    },
    {
      "Sid": "VisualEditor2",
      "Effect": "Allow",
      "Action": [
        "secretsmanager:GetResourcePolicy",
        "secretsmanager:GetSecretValue",
        "secretsmanager:DescribeSecret",
        "s3:*",
        "logs:*",
        "dynamodb:*",
        "execute-api:Invoke",
        "secretsmanager:ListSecretVersionIds"
      ],
      "Resource": [
        "arn:aws:dynamodb:*:311842024294:table/boxevents",
        "arn:aws:execute-api:eu-central-1:311842024294:w5ly8horsb/*/*/*",
        "arn:aws:execute-api:eu-central-1:311842024294:zzn85r98bi/*/*/*",
        "arn:aws:logs:eu-central-1:311842024294:log-group:boxlogs*",
        "arn:aws:secretsmanager:*:311842024294:secret:boxSecrets",
        "arn:aws:s3:::*myapp.cafe*"
      ]
    }
  ]
}